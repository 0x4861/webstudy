{
    "manifest_version": 2,
    "name": "Dorder",
    "version": "1.0.0",
    "description": "주문테스트 도구",
    "commands": {
        "toggle-key-command": {
            "suggested_key": {
                "default": "Ctrl+Shift+1",
                "mac": "Command+Shift+1"
            },
            "description": "key command",
            "global": true
        }
    },
    "browser_action": {
        "default_title": "Dorder",
        "default_popup": "pages/popup.html"
    },
    "author": "",
    "background": {
        "page": "pages/background.html"
    },
    "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["pages/index.js"],
        "run_at": "document_end"
    }
    ],
    "web_accessible_resources": ["pages/pagescript.js"],
    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>",
        "http://*/*",
        "https://*/*",
        "storage",
        "alarms"
    ],
    "minimum_chrome_version": "60"
}